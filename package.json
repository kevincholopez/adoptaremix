{
  "name": "adoptalaplataremix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npm run build:css",
    "dev": "npm run dev:css",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc",
    "dev:css": "concurrently \"tailwindcss -w -i styles.css -o app/styles/style.css\" \"remix dev\" ",
    "build:css": "concurrently \"tailwindcss -m -i styles.css -o app/styles/style.css\" \"remix build\" "
  },
  "dependencies": {
    "@remix-run/css-bundle": "^2.6.0",
    "@remix-run/node": "^2.6.0",
    "@remix-run/react": "^2.6.0",
    "@remix-run/serve": "^2.6.0",
    "@types/react-dropzone": "^5.1.0",
    "autoprefixer": "^10.4.17",
    "basic-ftp": "^5.0.5",
    "concurrently": "^8.2.2",
    "firebase": "^10.10.0",
    "flowbite-react": "^0.7.2",
    "formidable": "^3.5.1",
    "isbot": "^4.1.0",
    "mysql2": "^3.9.1",
    "postcss": "^8.4.35",
    "pure-react-carousel": "^1.30.1",
    "react": "^18.2.0",
    "react-alice-carousel": "^2.9.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-icons": "^5.0.1",
    "react-multi-carousel": "^2.8.4",
    "tailwindcss": "^3.4.1",
    "twilio": "^3.84.1",
    "uuid": "^9.0.1",
    "video-react": "^0.16.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.6.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
